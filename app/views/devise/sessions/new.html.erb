<%= stylesheet_link_tag 'login.css' %>
<%= stylesheet_link_tag "style.css", media: "all" %>

<div class="parent">

  <table cellpadding="0" cellspacing="0">
  <tbody>
    <tr>
      <td rowspan="3">&nbsp;</td>
      <td>
        <br>
        <table class="tbl_heading" cellpadding="0" cellspacing="0">
          <colgroup>
            <col width="10%">
            <col width="30%">
            <col width="36%">
            <col width="14%">
            <col width="5%">
            <col width="5%">
          </colgroup>
          <tbody>
            <tr>
              <td rowspan="5"> <%= image_tag "logo.png" %> </td>
              <td>&nbsp;</td>
              <td rowspan="5" colspan="6" class="txt_heading_right">  <span class="txt_mainheading"> Online Banking </span> </td>
            </tr>
            <tr>
              <td> <span class="txt_bottom txt_subheading"> Australia </span> </td>
            </tr>
          </tbody>
        </table>
        <br>
    <div class="container">
      <div class="login">
        <h2>Log in</h2>

        <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="field">
            <%= f.label :name %><br />
            <%= f.text_field :name, autofocus: true %>
          </div>

          <div class="field">
            <%= f.label :password %><br />
            <%= f.password_field :password, autocomplete: "off" %>
          </div>

          <div class="actions">
            <%= f.submit "Log in" %>
          </div>
        <% end %>
        <!-- <%= render "devise/shared/links" %> -->
      </div>
    </div>
  </div>
